<h1>Listing articles</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Content</th>
        <th></th>
    </tr>
</table>

    <% @articles.each do |article| %>
        <tr>
            <td><p></p><%= article.title %></td>
            <td><p></p><%= article.content %></td>
            <td><p></p><%= link_to 'Show', article_path(article) %></td>
        </tr>
    <% end %>
</table>

<%= link_to "New Article", new_article_path %>