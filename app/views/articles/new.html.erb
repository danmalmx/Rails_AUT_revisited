<%= form_with scope: :articles, local: true do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>

    <p>
        <%= f.label :content %><br>
        <%= f.text_field :content %>
    </p>

    <p>
        <%= f.submit name="Create Article"%>
    </p>
<% end %>